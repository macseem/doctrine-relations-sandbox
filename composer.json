{
  "name": "macseem/orm-test",
  "require":{
    "doctrine/orm": "^2.5.6",
    "symfony/yaml": "^3.2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1.3",
    "phpunit/dbunit": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "OrmTest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OrmTest\\": "tests/Unit",
      "OrmTest\\Test\\": "tests"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}